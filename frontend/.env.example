# Cricket Coach Booking App - Frontend Environment Variables
# Copy this file to .env for development or .env.production for production

# ===== API CONFIGURATION =====
# Backend API URL
VITE_API_URL=http://localhost:5000

# ===== PAYMENT PROCESSING =====
# StVITE_AVAILABLE_LANGUAGES=en,hiripe publishable key
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_publishable_key

# ===== AUTHENTICATION =====
# JWT token storage key
VITE_TOKEN_KEY=cricket_coach_token
# Refresh token storage key
VITE_REFRESH_TOKEN_KEY=cricket_coach_refresh_token

# ===== FEATURE FLAGS =====
# Enable notifications (true/false)
VITE_ENABLE_NOTIFICATIONS=true
# Enable dark mode (true/false)
VITE_ENABLE_DARK_MODE=true
# Enable analytics (true/false)
VITE_ENABLE_ANALYTICS=false

# ===== CACHING =====
# Cache duration in milliseconds
VITE_CACHE_DURATION=300000
# Maximum cache size (number of items)
VITE_MAX_CACHE_SIZE=100

# ===== RATE LIMITING =====
# API request throttle time in milliseconds
VITE_API_THROTTLE=2000
# Notification refresh interval in milliseconds
VITE_NOTIFICATION_REFRESH_INTERVAL=60000

# ===== LOCALIZATION =====
# Default language
VITE_DEFAULT_LANGUAGE=en
# Available languages (comma-separated)


# ===== MISCELLANEOUS =====
# Application name
VITE_APP_NAME=Cricket Coach Booking
# Application version
VITE_APP_VERSION=1.0.0
# Debug mode (true/false)
VITE_DEBUG=false 